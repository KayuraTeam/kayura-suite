		<!-- html -->
		<attribute>
			<name>id</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>classStyle</name>
		</attribute>
		<attribute>
			<name>style</name>
		</attribute>
		<!-- tree 属性 -->
		<attribute>
			<name>url</name>
			<description>检索远程数据的URL地址。</description>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>method</name>
			<description>检索数据的HTTP方法。（默认 POST / GET）</description>
		</attribute>
		<attribute>
			<name>animate</name>
